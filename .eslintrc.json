{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": ["error", "never"],
    "camelcase": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": "always",
        "ObjectPattern": "never"
      }
    ]
  }
}
