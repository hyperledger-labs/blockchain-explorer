{
  "network-configs": {
    "fabric-ca": {
      "version": "1.0",
      "clients": {
        "provider": {
          "tlsEnable": true,
          "organization": "provider",
          "channel": "mychannel",
          "credentialStore": {
            "path": "./tmp/fabric-client-kvs_provider",
            "cryptoStore": {
              "path": "./tmp/fabric-client-kvs_provider"
            }
          }
        }
      },
      "channels": {
        "mychannel": {
          "peers": {
            "peer1-consumer": {}
          },
          "connection": {
            "timeout": {
              "peer": {
                "endorser": "60000",
                "eventHub": "60000",
                "eventReg": "60000"
              }
            }
          }
        }
      },
      "orderers": {
        "orderer1-here": {
          "url": "grpcs://orderer1-here:7050"
        }
      },
      "organizations": {
        "here": {
          "mspid": "hereMSP",
          "fullpath": false,
          "adminPrivateKey": {
            "path": "/data/orgs/here/admin/msp/keystore"
          },
          "signedCert": {
            "path": "/data/orgs/here/admin/msp/signcerts"
          }
        },
        "consumer": {
          "name": "consumer",
          "mspid": "consumerMSP",
          "fullpath": false,
          "tlsEnable": true,
          "adminPrivateKey": {
            "path": "/data/orgs/consumer/admin/msp/keystore"
          },
          "signedCert": {
            "path": "/data/orgs/consumer/admin/msp/signcerts"
          }
        },
        "provider": {
          "name": "provider",
          "mspid": "providerMSP",
          "fullpath": false,
          "tlsEnable": true,
          "adminPrivateKey": {
            "path": "/data/orgs/provider/admin/msp/keystore"
          },
          "signedCert": {
            "path": "/data/orgs/provider/admin/msp/signcerts"
          }
        }
      },
      "peers": {
        "peer1-consumer": {
          "url": "grpcs://peer1-consumer:7051",
          "eventUrl": "grpcs://peer1-consumer:7053",
          "grpcOptions": {
            "ssl-target-name-override": "peer1-consumer"
          },
          "tlsCACerts": {
            "path": "/data/consumer-ca-chain.pem"
          }
        },
        "peer2-consumer": {
          "url": "grpcs://peer2-consumer:7051",
          "eventUrl": "grpcs://peer2-consumer:7053",
          "grpcOptions": {
            "ssl-target-name-override": "peer2-consumer"
          },
          "tlsCACerts": {
            "path": "/data/consumer-ca-chain.pem"
          }
        },
        "peer1-provider": {
          "url": "grpcs://peer1-provider:7051",
          "eventUrl": "grpcs://peer1-provider:7053",
          "grpcOptions": {
            "ssl-target-name-override": "peer1-provider"
          },
          "tlsCACerts": {
            "path": "/data/provider-ca-chain.pem"
          }
        },
        "peer2-provider": {
          "url": "grpcs://peer2-provider:7051",
          "eventUrl": "grpcs://peer2-provider:7053",
          "grpcOptions": {
            "ssl-target-name-override": "peer2-provider"
          },
          "tlsCACerts": {
            "path": "/data/provider-ca-chain.pem"
          }
        }
      }
    }
  }
}
