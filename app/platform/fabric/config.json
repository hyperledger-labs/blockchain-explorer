{
  "network-configs": {
    "fabric-ca": {
      "version": "1.0",
      "configType": "CA",
      "clients": {
        "governor": {
          "tlsEnable": true,
          "organization": "governor",
          "channel": "channel41",
          "credentialStore": {
            "path": "./tmp/fabric-client-kvs_governor",
            "cryptoStore": {
              "path": "./tmp/fabric-client-kvs_governor"
            }
          }
        }
      },
      "channels": {
        "channel41": {
          "peers": {
            "peer1-governor": {}
          },
          "connection": {
            "timeout": {
              "peer": {
                "endorser": "60000",
                "eventHub": "60000",
                "eventReg": "60000"
              }
            }
          }
        }
      },
      "orderers": {
        "": {
          "url": "grpcs://:7050"
        }
      },
      "organizations": {
        "blockchain-technology": {
          "mspid": "blockchain-technologyMSP",
          "fullpath": false,
          "adminPrivateKey": {
            "path": "/private/orgs/blockchain-technology/admin/msp/keystore"
          },
          "signedCert": {
            "path": "/private/orgs/blockchain-technology/admin/msp/signcerts"
          }
        },
        "governor": {
          "name": "governor",
          "mspid": "governorMSP",
          "fullpath": true,
          "tlsEnable": true,
          "adminPrivateKey": {
            "path": "./tmp/crypto/key.pem"
          },
          "signedCert": {
            "path": "./tmp/crypto/cert.pem"
          }
        }
      },
      "peers": {
        "peer1-governor": {
          "url": "grpcs://peer1-governor:7051",
          "eventUrl": "grpcs://peer1-governor:7053",
          "grpcOptions": {
            "ssl-target-name-override": "peer1-governor"
          },
          "tlsCACerts": {
            "path": "./tmp/crypto/ca.pem"
          }
        }
      }
    }
  }
}
